extends ../_default
include ../_mixins


block vars
	- var active = "Gallery"
	- var title = "Gallery 1"
	- var header_class = "page-header subpage_header"

block head

block header

	-var header_h1 = "Show Photos in a New Form"
	-var header_small = "Gallery"
	-var header_big = ""
	-var header_content = ""
	include ../includes/_header_transparent

block content
	section.section-border.text-center.text-lg-left
		.container
			ol.breadcrumb
				li: a(href="index.html") Home
				li: a(href="#") Gallery
				li.active: a(href="#") Gallery 1

	= '\n'
	//Start section
	section.text-center.well.well-sm
		.container
			.row
				.col-12
					h1 Grid with padding
					.lead Deliverables users, seamless beta-test implement tag.
				
				.col-12.margin-1
					.isotope-filters.isotope-filters-horizontal
						ul.inline-list.button-group-isotope.isotope-filters-list#isotope-filters
							li: a(href='#' data-isotope-filter="*" data-isotope-group="gallery").active.button.button-default.button-xs.round-xl All
							li: a(href='#' data-isotope-filter="type-1" data-isotope-group="gallery").button.button-default.button-xs.round-xl  Type 1
							li: a(href='#' data-isotope-filter="type-2" data-isotope-group="gallery").button.button-default.button-xs.round-xl  Type 2
							li: a(href='#' data-isotope-filter="type-3" data-isotope-group="gallery").button.button-default.button-xs.round-xl  Type 3

			.isotope(data-isotope-layout='fitRows', data-isotope-group='gallery' data-lightgallery="group")
				.row
					.col-6.col-md-6.col-lg-4.isotope-item(data-filter="type-1")
						.thumbnail-variant-2.thumbnail-4.text-center
